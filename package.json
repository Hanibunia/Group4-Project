{
  "name": "devops",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nyc": {
    "include": [
      "utils/UserUtil.js",
      "utils/ReviewUtil.js",
      "public/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "text-summary"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "scripts": {
    "test": "nyc mocha --timeout 20000 tests/*.test.js",
    "start": "node index.js",
    "frontend-instrument": "nyc instrument .\\public\\js .\\public\\instrumented\\js && copy .\\public\\*.html .\\public\\instrumented\\ && copy .\\public\\images .\\public\\instrumented\\ && copy .\\public\\css .\\public\\instrumented\\ && del.\\coverage-frontend\\*.json",
    "frontend-report": "nyc report --reporter html --reporter text -t coverage-frontend --report-dir coverage-frontend/summary"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "body-parser": "^1.20.2",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "chromedriver": "^120.0.1",
    "datadog-tracer": "^0.4.2",
    "dd-trace": "^5.2.0",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "fs": "^0.0.1-security",
    "joi": "^17.11.0",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "nyc": "^15.1.0",
    "pm2": "^5.3.1",
    "selenium-webdriver": "^4.16.0",
    "sinon": "^17.0.1",
    "winston": "^3.11.0"
  }
}
